@page
@model RacEquipe.Pages.Utilisateurs.DeleteModel

@{
    ViewData["Title"] = "Supprimer";
}

<h1>Supprimer</h1>

<h3>Êtes-vous certain de vouloir supprimer ceci ?</h3>
<div>
    <h4>Utilisateur</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Utilisateur.Nom)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Utilisateur.Nom)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Utilisateur.Prenom)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Utilisateur.Prenom)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Utilisateur.UtilisateurId" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-page="./Index">Retourner à la liste</a>
    </form>
</div>
